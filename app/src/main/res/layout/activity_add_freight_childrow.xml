<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/SingleProfile"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/selector_dashboard_list"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="5dp"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:gravity="center"
        tools:ignore="UnusedAttribute">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:weightSum="1"
            android:gravity="left"
            android:layout_gravity="center"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/txt_cust"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/customername"
                android:textSize="13dp"
                android:textStyle="bold"
                android:layout_weight="0.65"
                android:textColor="@color/primary_text"/>

            <TextView
                android:id="@+id/customer_value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="001"
                android:textSize="13dp"
                android:textStyle="bold"
                android:layout_weight="0.35"
                android:textColor="@color/primary_text"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:weightSum="1"
            android:gravity="left"
            android:layout_gravity="center"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/txt_eporder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/Order_no"
                android:textSize="13dp"
                android:textStyle="bold"
                android:layout_weight="0.83"
                android:textColor="@color/primary_text"/>

            <TextView
                android:id="@+id/eporder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="13dp"
                android:textStyle="bold"
                android:layout_weight="0.17"
                android:textColor="@color/primary_text"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:weightSum="3"
            android:gravity="center"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1.1"
                android:weightSum="1">



                <TextView
                    android:id="@+id/txt_ordered"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/po"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.5"
                    android:textColor="@color/primary_text"/>

                <TextView
                    android:id="@+id/order_value"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="12"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.5"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:weightSum="1">

                <TextView
                    android:id="@+id/txt_branch"
                    android:text="@string/branch_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.45"

                    android:textColor="@color/primary_text"/>
                <TextView
                    android:id="@+id/branch_value"
                    android:text="001"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.55"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".9"
                android:weightSum="1">

                <TextView
                    android:id="@+id/txt_weight"
                    android:text="@string/weight"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.45"
                    android:textColor="@color/primary_text"/>
                <TextView
                    android:id="@+id/weight_value"
                    android:text="88"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.55"
                    android:gravity="left"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:weightSum="3"
            android:gravity="center"
            android:layout_gravity="center"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1.1"
                android:weightSum="1">


                <TextView
                    android:id="@+id/txt_value"
                    android:text="@string/value"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.5"
                    android:textColor="@color/primary_text"/>

                <TextView
                    android:id="@+id/value"
                    android:text="88"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.5"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:weightSum="1">

                <TextView
                    android:id="@+id/txt_indound"
                    android:text="@string/inbound"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.45"
                    android:textColor="@color/primary_text"/>
                <TextView
                    android:id="@+id/inbound_value"
                    android:text="00"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.55"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".9"
                android:weightSum="1">
                <TextView
                    android:id="@+id/txt_outbound"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/outbound"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.45"
                    android:textColor="@color/primary_text"/>
                <TextView
                    android:id="@+id/outbound_value"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="00"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:layout_weight="0.55"
                    android:gravity="left"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>